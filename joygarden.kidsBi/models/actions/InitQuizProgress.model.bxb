action (InitQuizProgress) {
  type (BeginTransaction)
  description (Create quiz progress)
  collect {
    input (quizInfos) {
      type (QuizInfo)
      min (Required) max (Many)
      default-init {
        intent {
          goal: GetQuiz
          value: QuizCategory(Korean)
        }
      }
    }
  }
  output (QuizProgress)
}
