action (GetVideo) {
  description (Action to search folktale by searchTerm or category)
  type (Search)
  collect {
    input (searchTerm) {
      type (SearchTerm)
      min (Optional) max (One)
    }
    input (category) {
      type (VideoCategory)
      min (Optional) max (One)
    }
  }
  output (VideoInfo)
}
