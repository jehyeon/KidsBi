structure (VideoInfo) {
  description (Folktale info as output of GetVideo action)
  // 모든 property가 있는 데이터만 사용
  property (title) {
    type (VideoTitle)
    min (Required) max (One)
  }

  property (videoId) {
    type (VideoId)
    min (Required) max (One)
  }

  property (categories) {
    type (VideoCategory)
    min (Required) max (Many)
  }

  property (like) {
    type (VideoLike)
    min (Required) max (One)
  }

  property (source) {
    type (VideoSource)
    min (Required) max (One)
  }

  property (thumbnail) {
    type (VideoThumbnail)
    min (Optional) max (One)
  }

  property (viewCount) {
    type (VideoViewCount)
    min (Required) max (One)
  }
}
