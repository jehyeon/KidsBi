dialog (Value) {
  match: QuizCategory (quizCategory)

  if (quizCategory == 'Animal') {
    template ("동물 퀴즈")
  } else-if (quizCategory == 'Flower') {
      template ("꽃 퀴즈")
  } else-if (quizCategory == 'Fruit') {
      template ("과일 퀴즈")
  } else-if (quizCategory == 'Math') {
      template ("수학 퀴즈")
  } else-if (quizCategory == 'Country') {
      template ("나라 퀴즈")
  } else-if (quizCategory == 'Person') {
      template ("인물 퀴즈")
  }
}
