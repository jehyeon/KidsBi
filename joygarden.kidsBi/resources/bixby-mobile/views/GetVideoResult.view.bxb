result-view {
	match: CategoryInfo (categoryInfos) {
		from-output: GetCategory
	}
	
	message {
		template-macro (kidsbi_1_0_Search_Result)
	}
	
	render {
		if (size(categoryInfos) > 1) {
			layout-macro (categoryInfosMacro) {
				param (categoryInfos) {
					expression (categoryInfos)
				}
			}
		}
	}
	conversation-drivers {
		
		conversation-driver {
			template-macro(ID_Search_Princess)
		}
	}
}
