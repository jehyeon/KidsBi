input-view {
  match: CategoryInfo (categoryInfos) {
    to-input: SelectVideoCategory
  }

  message {
    template ("어느 동화가 보고 싶어요?") // 임시
  }

  render {
    selection-of (categoryInfos) {
      where-each (categoryInfo) {
        layout-match (categoryInfo) {
          mode (Summary)
        }
      }
    }
  }
}